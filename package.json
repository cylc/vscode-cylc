{
    "name": "cylc",
    "displayName": "Cylc",
    "description": "Language support for Cylc workflow configuration files",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "cylc",
            "aliases": ["Cylc", "cylc"],
            "extensions": [".rc"],
            "configuration": "./cylc.language-configuration.json"
        }],
        "grammars": [{
            "language": "cylc",
            "scopeName": "source.cylc",
            "path": "./syntaxes/cylc.tmLanguage.json"
        }]
    }
}